<Window x:Class="GMCWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GMCWPF"
        mc:Ignorable="d"
        Title="工数コメントをいい感じに整形するやつ" Height="400" Width="660"
        Background="AliceBlue" MaxWidth="660">

    <Window.Resources>
 
    </Window.Resources>

    <Grid Margin="10,10,10,10">
        <Grid.RowDefinitions>
            <RowDefinition Height="1"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>

        <Button Content="完了" Grid.Column="1" HorizontalAlignment="Right" Margin="0,10,10,0" Grid.Row="2" Width="75" Click="CompleteBtn_Click"/>

        <TreeView Name="treeView" Background="Black" Margin="0,0,0,0" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2">
            <!--<local:DockPanelMain/>-->
        </TreeView>
    </Grid>
</Window>
